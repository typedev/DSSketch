family Unknown

axes
    wght 1:400:1000 "Weight"
    wdth 50:100:150 "Width"
    opsz 6:16:144 "Optical size"

avar2 vars
    $wght1 = 600

avar2 matrix
    outputs                  wght    wdth 
    [wght=1000, wdth=50]     $wght1  50   
    [wght=1000, wdth=90]     $wght1  90   
    [wght=600, wdth=50]      $wght1  50   
    [wght=600, wdth=90]      $wght1  90   
    [wght=1000, wdth=90.02]  1000    90.02

instances off